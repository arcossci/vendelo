<%= form_for @product do |f| %>
  <% @product.errors.full_messages.each do |error| %>
  <div>
    <%= error %>
  </div>
  <%end%>
  <%= f.label 'Product Title' %><br>
  <%= f.text_field :title %><br>

  <%= f.label 'Product Description' %><br>
  <%= f.text_area :desc_text %><br>

  <%= f.submit "Submit" %>
<%end%>
